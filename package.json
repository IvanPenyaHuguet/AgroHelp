{
  "name": "agrohelper",
  "author": "ivanpenyahuguet",
  "version": "1.0.0",
  "description": "An application to help on spain agriculture",
  "main": "./.webpack/main/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "electron-forge publish",
    "lint": "eslint . --ext js,ts",
    "test": "jest",
    "cypress:open": "cypress open"
  },
  "keywords": [],
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.7",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/lab": "^5.0.0-alpha.59",
    "@mui/material": "^5.2.3",
    "@mui/styles": "^5.2.3",
    "@mui/x-data-grid": "^5.1.0",
    "chart.js": "^3.4.0",
    "chartjs-adapter-dayjs-3": "^1.2.2",
    "clsx": "^1.1.1",
    "crypto-browserify": "^3.12.0",
    "dayjs": "^1.10.5",
    "formik": "^2.2.9",
    "formik-mui": "^4.0.0-alpha.3",
    "formik-mui-lab": "^1.0.0-alpha.3",
    "node-polyfill-webpack-plugin": "^1.1.3",
    "pouchdb-adapter-idb": "^7.2.2",
    "react": "17.0.2",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.0",
    "react-number-format": "^4.6.3",
    "rxdb": "^13.15.3",
    "rxdb-hooks": "^4.0.0",
    "rxjs": "^7.1.0",
    "styled-components": "5.3.0",
    "uuid": "^8.3.2",
    "wouter": "^2.7.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@cypress/react": "^5.9.1",
    "@cypress/webpack-dev-server": "^1.7.0",
    "@electron-forge/cli": "6.0.0-beta.61",
    "@electron-forge/maker-deb": "6.0.0-beta.61",
    "@electron-forge/maker-rpm": "6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "6.0.0-beta.61",
    "@electron-forge/maker-zip": "6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "6.0.0-beta.61",
    "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@types/crypto-js": "^4.0.1",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/jest": "26.0.23",
    "@types/leveldown": "^4.0.2",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/styled-components": "5.1.10",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "babel-loader": "8.2.2",
    "cross-env": "7.0.3",
    "cypress": "^7.6.0",
    "electron": "16.0.4",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-standard": "5.0.0",
    "express-pouchdb": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "27.0.6",
    "node": "^16.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "ts-jest": "27.0.3",
    "typescript": "4.5.2",
    "wait-on": "5.3.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "AgroHelp",
        "executableName": "AgroHelp",
        "icon": "assets/icon",
        "extraResource": [
          "assets"
        ]
      },
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack/main.webpack.js",
            "renderer": {
              "config": "./webpack/renderer.webpack.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./electron/bridge.ts"
                  }
                }
              ]
            },
            "port": 3000,
            "loggerPort": 3001
          }
        ]
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "AgroHelp"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "browser": {
    "crypto": false,
    "fs": false,
    "path": false,
    "os": false,
    "stream": false,
    "http": false,
    "tls": false,
    "zlib": false,
    "https": false,
    "net": false
  },
  "resolve": {
    "fallback": {
      "crypto": false
    }
  }
}
